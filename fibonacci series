#include<iostream>
using namespace std;
int main()
{
  int n;
  cout<<"enter the number";
  cin>>n;
  int t1=0,t2=1,next;
  cout<<"fibonacci series\n";
  for (int i=0;i<n;i++)
  {
    if(i<=1)
      next=i;
    else
    {
      next=t1+t2;
      t1=t2;
      t2=next;
    }
    cout<<next<<" ";
  }
  cout<<ends;
  return 0;
}
