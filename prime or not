#include<iostream>
using namespace std;
int main()
{
  int n,m=0,i,flag=0;
  cout<<"enter the number\n";
  cin>>n;
  m=n/2;
  for(i=2;i<=m;i++)
  {
    if(n%i==0)
    {
      cout<<"number is not prime";
      flag=1;
      break;
    }
  }
    if(flag==0)
      cout<<"number is prime";
  return 0;
}
  
